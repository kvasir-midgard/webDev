<% include partials/header %>

<h1>The posts page</h1>

<% for(var i = 0; i < posts.length; i++){ %>
    <li> 
        Title: <%= posts[i].title %> 
        Author: <strong><%= posts[i].author %></strong>
    </li>
<% } %>

<br>
<br>

<% posts.forEach(function(post){ %>
    <li> 
        Title: <%= post.title %> 
        Author: <strong><%= post.author %></strong>
    </li>
<% }); %>
<% include partials/footer %>
